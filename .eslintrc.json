{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "no-tabs": 0,
        "linebreak-style":[
            "error",
            "unix"
        ],
        "no-underscore-dangle":"off"
    }
}
